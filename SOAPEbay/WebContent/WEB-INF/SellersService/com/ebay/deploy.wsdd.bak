<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from SellersService WSDL service -->

  <service name="Sellers" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://ebay.com"/>
      <parameter name="wsdlServiceElement" value="SellersService"/>
      <parameter name="schemaQualified" value="http://ebay.com"/>
      <parameter name="wsdlServicePort" value="Sellers"/>
      <parameter name="className" value="com.ebay.SellersSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="Sellers"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="loadCategory" qname="operNS:loadCategory" xmlns:operNS="http://ebay.com" soapAction="" >
      </operation>
      <parameter name="allowedMethods" value="loadCategory"/>

  </service>
</deployment>
